//--------    FILE: depth.bhv   -------------

initialize   DEPLOY = false

//----------------------------------------------
Behavior = BHV_GoToDepth
{
  name         = gotodepth_profile
  pwt          = 100
  condition    = DEPLOY = true
  condition    = RETURN = false

  capture_delta = 1

  // 40m停20s -> 30m停10s -> 20m停15s -> 回到1m停5s
  depth        = 40,20:30,10:20,15:1,5
  repeat       = 0

  // 结束后触发结束任务
  endflag   = DEPLOY = false
  endflag   = MISSION = complete
}
